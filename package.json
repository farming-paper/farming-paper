{
  "name": "farming-paper",
  "version": "0.0.1",
  "description": "문제를 직접 디자인하고 최적화된 루틴으로 출제되는 문제를 풀어보세요.",
  "main": "index.js",
  "sideEffects": false,
  "scripts": {
    "test": "vitest",
    "test:ci": "vitest run",
    "main": "vite-node ./app/main.ts",
    "build": "npm run build:css && remix build --sourcemap",
    "dev": "concurrently -c bgBlue.bold,bgCyan.bold \"npm run dev:css\" \"remix dev\"",
    "build:css": "tailwindcss -m -i ./styles/app.css -o app/styles/app.css",
    "dev:css": "tailwindcss -w -i ./styles/app.css -o app/styles/app.css",
    "lint": "eslint .",
    "type-check": "tsc --noEmit",
    "gen:sb": "supabase status && cat app/supabase/typegen-prefix.txt > app/supabase/generated/supabase-types.ts && supabase gen types typescript --local >> app/supabase/generated/supabase-types.ts && prettier --write app/supabase/generated/supabase-types.ts",
    "gen": "concurrently --names js -c bgBlue.bold 'pnpm gen:sb'",
    "sb-diff": "supabase db diff --use-migra -f",
    "sb-push": "supabase db push"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@ant-design/icons": "^5.1.4",
    "@floating-ui/react": "^0.24.2",
    "@headlessui/react": "^1.7.15",
    "@heroicons/react": "^2.0.18",
    "@rehookify/datepicker": "^5.2.1",
    "@remix-run/node": "^1.17.0",
    "@remix-run/react": "^1.17.0",
    "@remix-run/serve": "^1.17.0",
    "@remix-run/server-runtime": "^1.17.0",
    "@remix-run/vercel": "^1.17.0",
    "@remix-validated-form/with-zod": "^2.0.5",
    "@supabase/auth-helpers-remix": "^0.2.1",
    "@supabase/supabase-js": "^2.24.0",
    "@tailwindcss/forms": "^0.5.3",
    "@toss/hangul": "^1.3.0",
    "@types/inquirer": "^9.0.3",
    "@vercel/analytics": "^1.0.1",
    "@vercel/node": "^2.15.1",
    "@vitejs/plugin-react": "^4.0.0",
    "antd": "^5.6.1",
    "compromise": "^14.9.0",
    "dayjs": "^1.11.8",
    "framer-motion": "^10.12.16",
    "inquirer": "^9.2.7",
    "katex": "^0.16.7",
    "lru-cache": "^9.1.2",
    "lucide-react": "^0.241.0",
    "marked": "^5.1.0",
    "nanoid": "^4.0.2",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.44.3",
    "react-select": "^5.7.3",
    "remix-auth": "^3.4.0",
    "remix-auth-google": "^1.2.0",
    "remix-validated-form": "^4.6.12",
    "rfdc": "^1.3.0",
    "tailwind-merge": "^1.13.1",
    "type-fest": "^3.11.1",
    "usehooks-ts": "^2.9.1",
    "vite": "^4.3.9",
    "vite-node": "^0.32.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@babel/core": "^7.22.5",
    "@remix-run/dev": "^1.17.0",
    "@remix-run/eslint-config": "^1.17.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/katex": "^0.16.0",
    "@types/marked": "^5.0.0",
    "@types/node": "^18.16.16",
    "@types/react": "^18.2.9",
    "@types/react-dom": "^18.2.4",
    "@typescript-eslint/eslint-plugin": "^5.59.9",
    "@typescript-eslint/parser": "^5.59.9",
    "autoprefixer": "^10.4.14",
    "concurrently": "^8.2.0",
    "eslint": "^8.42.0",
    "happy-dom": "^9.20.3",
    "jsdom": "^22.1.0",
    "postcss": "^8.4.24",
    "tailwindcss": "^3.3.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3",
    "vitest": "^0.32.0"
  }
}
