{
  "name": "farming-paper",
  "version": "0.0.1",
  "description": "문제를 직접 디자인하고 최적화된 루틴으로 출제되는 문제를 풀어보세요.",
  "main": "index.js",
  "sideEffects": false,
  "scripts": {
    "test": "vitest",
    "test:ci": "vitest run",
    "main": "vite-node ./app/main.ts",
    "build": "npm run build:css && remix build --sourcemap",
    "dev": "concurrently -c bgBlue.bold,bgCyan.bold \"npm run dev:css\" \"remix dev\"",
    "build:css": "tailwindcss -m -i ./styles/app.css -o app/styles/app.css",
    "dev:css": "tailwindcss -w -i ./styles/app.css -o app/styles/app.css",
    "lint": "eslint .",
    "type-check": "tsc --noEmit",
    "gen:sb": "cat app/supabase/typegen-prefix.txt > app/supabase/generated/supabase-types.ts && supabase gen types typescript --local >> app/supabase/generated/supabase-types.ts && prettier --write app/supabase/generated/supabase-types.ts",
    "gen": "concurrently --names js -c bgBlue.bold 'pnpm gen:sb'",
    "sb-diff": "supabase db diff --use-migra -f",
    "sb-push": "supabase db push"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@ant-design/icons": "^5.0.1",
    "@headlessui/react": "^1.7.14",
    "@heroicons/react": "^2.0.17",
    "@remix-run/node": "^1.15.0",
    "@remix-run/react": "^1.15.0",
    "@remix-run/serve": "^1.15.0",
    "@remix-run/server-runtime": "^1.15.0",
    "@remix-run/vercel": "^1.15.0",
    "@supabase/auth-helpers-remix": "^0.1.9",
    "@supabase/supabase-js": "^2.21.0",
    "@toss/hangul": "^1.3.0",
    "@types/inquirer": "^9.0.3",
    "@vercel/analytics": "^0.1.11",
    "@vercel/node": "^2.12.0",
    "@vitejs/plugin-react": "^3.1.0",
    "antd": "^5.4.5",
    "compromise": "^14.8.2",
    "dayjs": "^1.11.7",
    "framer-motion": "^10.12.4",
    "inquirer": "^9.2.0",
    "katex": "^0.16.6",
    "lru-cache": "^8.0.5",
    "lucide-react": "^0.180.0",
    "marked": "^4.3.0",
    "nanoid": "^4.0.2",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.9",
    "react-select": "^5.7.2",
    "remix-auth": "^3.4.0",
    "remix-auth-google": "^1.2.0",
    "rfdc": "^1.3.0",
    "tailwind-merge": "^1.12.0",
    "type-fest": "^3.9.0",
    "usehooks-ts": "^2.9.1",
    "vite": "^4.3.2",
    "vite-node": "^0.30.1"
  },
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@remix-run/dev": "^1.15.0",
    "@remix-run/eslint-config": "^1.15.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/katex": "^0.16.0",
    "@types/marked": "^4.0.8",
    "@types/node": "^18.16.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.1",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "autoprefixer": "^10.4.14",
    "concurrently": "^8.0.1",
    "eslint": "^8.39.0",
    "happy-dom": "^8.9.0",
    "jsdom": "^21.1.1",
    "postcss": "^8.4.23",
    "tailwindcss": "^3.3.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4",
    "vitest": "^0.30.1"
  }
}
