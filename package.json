{
  "name": "farming-paper",
  "version": "0.0.1",
  "description": "문제를 직접 디자인하고 최적화된 루틴으로 출제되는 문제를 풀어보세요.",
  "main": "index.js",
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "test": "vitest",
    "test:ci": "vitest run",
    "start": "remix-serve ./build/index.js",
    "build": "remix build --sourcemap && prisma generate",
    "format": "prettier --write .",
    "dev": "remix dev --manual",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "type-check": "tsc --noEmit",
    "gen:sb": "supabase status && cat app/supabase/typegen-prefix.txt > app/supabase/generated/supabase-types.ts && supabase gen types typescript --local >> app/supabase/generated/supabase-types.ts && prettier --write app/supabase/generated/supabase-types.ts",
    "gen:prisma": "prisma generate",
    "gen": "concurrently --names js -c bgBlue.bold 'pnpm gen:sb'",
    "sb-diff": "supabase db diff --use-migra -f",
    "sb-push": "supabase db push"
  },
  "eslintIgnore": [
    "/node_modules",
    "/build",
    "/public/build"
  ],
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@floating-ui/react": "^0.26.4",
    "@headlessui/react": "^1.7.17",
    "@heroicons/react": "^2.1.1",
    "@nextui-org/react": "^2.2.9",
    "@prisma/client": "^5.7.1",
    "@rehookify/datepicker": "^6.5.0",
    "@remix-run/css-bundle": "^2.4.1",
    "@remix-run/node": "^2.4.1",
    "@remix-run/react": "^2.4.1",
    "@remix-run/serve": "^2.4.1",
    "@remix-run/server-runtime": "^2.4.1",
    "@remix-validated-form/with-zod": "^2.0.7",
    "@supabase/auth-helpers-remix": "^0.2.6",
    "@supabase/supabase-js": "^2.39.1",
    "@tailwindcss/forms": "^0.5.7",
    "@toss/hangul": "^1.6.1",
    "@vercel/analytics": "^1.1.1",
    "@vercel/remix": "^2.4.1",
    "@vitejs/plugin-react": "^4.2.1",
    "compromise": "^14.11.0",
    "dayjs": "^1.11.10",
    "framer-motion": "^10.16.16",
    "jotai": "^2.6.0",
    "katex": "^0.16.9",
    "lru-cache": "^10.1.0",
    "lucide-react": "^0.302.0",
    "marked": "^11.1.0",
    "nanoid": "^5.0.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.49.2",
    "react-select": "^5.8.0",
    "remix-validated-form": "^5.1.5",
    "rfdc": "^1.3.0",
    "tailwind-merge": "^2.2.0",
    "type-fest": "^4.9.0",
    "usehooks-ts": "^2.9.1",
    "vite": "^5.0.10",
    "vite-node": "^1.1.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@babel/core": "^7.23.6",
    "@remix-run/dev": "^2.4.1",
    "@remix-run/eslint-config": "^2.4.1",
    "@testing-library/jest-dom": "^6.1.6",
    "@testing-library/react": "^14.1.2",
    "@types/katex": "^0.16.7",
    "@types/node": "^20.10.5",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^6.16.0",
    "@typescript-eslint/parser": "^6.16.0",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.2",
    "eslint": "^8.56.0",
    "happy-dom": "^12.10.3",
    "jsdom": "^23.0.1",
    "postcss": "^8.4.32",
    "prisma": "^5.7.1",
    "react-router-dom": "^6.21.1",
    "tailwindcss": "^3.4.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "vitest": "^1.1.0"
  }
}
