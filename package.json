{
  "name": "farming-paper",
  "version": "0.0.1",
  "description": "문제를 직접 디자인하고 최적화된 루틴으로 출제되는 문제를 풀어보세요.",
  "main": "index.js",
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "test": "vitest",
    "test:ci": "vitest run",
    "start": "remix-serve ./build/index.js",
    "build": "remix build --sourcemap && prisma generate",
    "format": "prettier --write .",
    "dev": "remix dev --manual",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "typecheck": "tsc --noEmit",
    "gen:sb": "supabase status && cat app/supabase/typegen-prefix.txt > app/supabase/generated/supabase-types.ts && supabase gen types typescript --local >> app/supabase/generated/supabase-types.ts && prettier --write app/supabase/generated/supabase-types.ts",
    "gen:prisma": "prisma generate",
    "gen": "concurrently --names js -c bgBlue.bold 'pnpm gen:sb'",
    "sb-diff": "supabase db diff --use-migra -f",
    "sb-push": "supabase db push"
  },
  "eslintIgnore": [
    "/node_modules",
    "/build",
    "/public/build"
  ],
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@floating-ui/react": "^0.26.13",
    "@headlessui/react": "^1.7.19",
    "@heroicons/react": "^2.1.3",
    "@nextui-org/react": "^2.3.6",
    "@prisma/client": "^5.13.0",
    "@rehookify/datepicker": "^6.6.1",
    "@remix-run/css-bundle": "2.9.1",
    "@remix-run/node": "2.9.1",
    "@remix-run/react": "2.9.1",
    "@remix-run/serve": "2.9.1",
    "@remix-run/server-runtime": "2.9.1",
    "@remix-validated-form/with-zod": "^2.0.7",
    "@supabase/auth-helpers-remix": "^0.4.0",
    "@supabase/supabase-js": "^2.42.7",
    "@tailwindcss/forms": "^0.5.7",
    "@toss/hangul": "^1.7.0",
    "@vercel/analytics": "^1.2.2",
    "@vercel/remix": "2.9.0",
    "@vitejs/plugin-react": "^4.2.1",
    "compromise": "^14.13.0",
    "dayjs": "^1.11.11",
    "fast-equals": "^5.0.1",
    "framer-motion": "^11.1.7",
    "immer": "^10.1.1",
    "is-hotkey": "^0.2.0",
    "isbot": "5.1.5",
    "jotai": "^2.8.0",
    "katex": "^0.16.10",
    "lru-cache": "^10.2.2",
    "lucide-react": "^0.376.0",
    "marked": "^12.0.2",
    "nanoid": "^5.0.7",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.51.3",
    "react-select": "^5.8.0",
    "react-toastify": "^10.0.5",
    "remix-validated-form": "^5.1.5",
    "rfdc": "^1.3.1",
    "slate": "^0.103.0",
    "slate-history": "^0.100.0",
    "slate-hyperscript": "^0.100.0",
    "slate-react": "^0.102.0",
    "tailwind-merge": "^2.3.0",
    "type-fest": "^4.17.0",
    "usehooks-ts": "^3.1.0",
    "vite": "^5.2.10",
    "zod": "^3.23.4"
  },
  "devDependencies": {
    "@babel/core": "^7.24.4",
    "@remix-run/dev": "2.9.1",
    "@remix-run/eslint-config": "2.9.1",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^15.0.5",
    "@types/is-hotkey": "^0.1.10",
    "@types/katex": "^0.16.7",
    "@types/node": "^20.12.7",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.7.1",
    "@typescript-eslint/parser": "^7.7.1",
    "autoprefixer": "^10.4.19",
    "concurrently": "^8.2.2",
    "eslint": "^9.1.1",
    "happy-dom": "^14.7.1",
    "jsdom": "^24.0.0",
    "postcss": "^8.4.38",
    "prisma": "^5.13.0",
    "react-router-dom": "^6.23.0",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.4.5",
    "vitest": "^1.5.2"
  }
}
