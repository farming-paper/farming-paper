{
  "name": "farming-paper",
  "version": "0.0.1",
  "description": "문제를 직접 디자인하고 최적화된 루틴으로 출제되는 문제를 풀어보세요.",
  "main": "index.js",
  "sideEffects": false,
  "scripts": {
    "test": "vitest",
    "test:ci": "vitest run",
    "main": "vite-node ./app/main.ts",
    "build": "npm run build:css && remix build --sourcemap",
    "dev": "concurrently -c bgBlue.bold,bgCyan.bold \"npm run dev:css\" \"remix dev\"",
    "build:css": "tailwindcss -m -i ./styles/app.css -o app/styles/app.css",
    "dev:css": "tailwindcss -w -i ./styles/app.css -o app/styles/app.css",
    "lint": "eslint .",
    "type-check": "tsc --noEmit",
    "gen:sb": "cat app/supabase/typegen-prefix.txt > app/supabase/generated/supabase-types.ts && supabase gen types typescript --local >> app/supabase/generated/supabase-types.ts && prettier --write app/supabase/generated/supabase-types.ts",
    "gen:sb-graphql": "graphql-codegen --config app/supabase/graphql-typegen-config.js",
    "gen": "concurrently --names js -c bgBlue.bold 'pnpm gen:sb'",
    "sb-diff": "supabase db diff --use-migra -f",
    "sb-push": "supabase db push"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@ant-design/icons": "^4.8.0",
    "@headlessui/react": "^1.7.7",
    "@heroicons/react": "^2.0.13",
    "@remix-run/node": "^1.10.0",
    "@remix-run/react": "^1.10.0",
    "@remix-run/server-runtime": "^1.10.0",
    "@remix-run/vercel": "^1.10.0",
    "@supabase/auth-helpers-remix": "^0.1.3",
    "@supabase/supabase-js": "^2.2.3",
    "@types/inquirer": "^9.0.3",
    "@vercel/analytics": "^0.1.6",
    "@vercel/node": "^2.8.5",
    "@vitejs/plugin-react": "^3.0.1",
    "antd": "^5.1.4",
    "compromise": "^14.8.1",
    "dayjs": "^1.11.7",
    "framer-motion": "^8.3.3",
    "graphql": "^16.6.0",
    "inquirer": "^9.1.4",
    "katex": "^0.16.4",
    "lucide-react": "^0.104.1",
    "marked": "^4.2.5",
    "nanoid": "^4.0.0",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.41.5",
    "remix-auth": "^3.4.0",
    "remix-auth-google": "^1.2.0",
    "rfdc": "^1.3.0",
    "tailwind-merge": "^1.8.1",
    "type-fest": "^3.5.1",
    "usehooks-ts": "^2.9.1",
    "vite": "^4.0.4",
    "vite-node": "^0.27.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@graphql-codegen/cli": "^2.16.3",
    "@remix-run/dev": "^1.10.0",
    "@remix-run/eslint-config": "^1.10.0",
    "@remix-run/serve": "^1.10.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/katex": "^0.16.0",
    "@types/marked": "^4.0.8",
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.48.1",
    "autoprefixer": "^10.4.13",
    "concurrently": "^7.6.0",
    "eslint": "^8.31.0",
    "happy-dom": "^8.1.3",
    "jsdom": "^21.0.0",
    "postcss": "^8.4.21",
    "tailwindcss": "^3.2.4",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4",
    "vitest": "^0.27.0"
  }
}
